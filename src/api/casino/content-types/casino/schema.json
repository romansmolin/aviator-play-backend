{
  "kind": "collectionType",
  "collectionName": "casinos",
  "info": {
    "singularName": "casino",
    "pluralName": "casinos",
    "displayName": "Casino",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "bonus_title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "logo": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "uuid": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid"
    },
    "features": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "options": [
        "crypto_casino",
        "vip_available",
        "no_deposit_bonus_available",
        "support_available",
        "valid_license",
        "aviator_game_available",
        ""
      ]
    },
    "rating": {
      "type": "integer",
      "max": 100,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "review": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Promos": {
      "type": "component",
      "repeatable": true,
      "component": "list.available-promos-list",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "bonuses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::bonus.bonus",
      "mappedBy": "casinos"
    },
    "faq": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "faq.faq"
    },
    "mainBonus": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "bonus.main-bonus-info"
    },
    "casinoType": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "customField",
      "options": [
        "Pay N Play Casino:pay-n-play",
        "Crypto Casino:crypto-casino",
        "Fresh Casino:fresh-casino",
        "Sportsbook:sportsbook"
      ],
      "customField": "plugin::multi-select.multi-select"
    }
  }
}
