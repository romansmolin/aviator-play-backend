{
  "kind": "collectionType",
  "collectionName": "bonuses",
  "info": {
    "singularName": "bonus",
    "pluralName": "bonuses",
    "displayName": "Bonus",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "casinos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::casino.casino",
      "inversedBy": "bonuses"
    },
    "bonus_info": {
      "type": "component",
      "repeatable": false,
      "component": "bonus.bonus-info",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "bonus_subtitle": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "logo": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "bonusOverview": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "bonusTitle": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "casinoName": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "uuid": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid"
    },
    "faq": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "faq.faq"
    },
    "mainBonusTypeForUrl": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "customField",
      "options": [
        "No Deposit Bonus:no-deposit-bonus",
        "Welcome Bonus:welcome-bonus",
        "Cashback Bonus:cashback-bonus"
      ],
      "customField": "plugin::multi-select.multi-select"
    }
  }
}
